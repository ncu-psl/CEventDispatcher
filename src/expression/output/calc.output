    7     | '!' expr
    8     | expr '+' expr
    9     | expr '-' expr
   10     | expr '*' expr
   11     | expr '/' expr
   12     | expr '%' expr
   13     | expr '<' expr
   14     | expr '>' expr
   15     | expr AND expr
   16     | expr OR expr
   17     | expr GE expr
   18     | expr LE expr
   19     | expr NE expr
   20     | expr EQ expr
   21     | '(' expr ')'


